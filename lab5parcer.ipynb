    {
      "cell_type": "code",
      "source": [
        "pageRequest = requests.get(\"https://store.steampowered.com/search/?filter=globaltopsellers&os=win\")\n",
        "pageSource = BeautifulSoup(pageRequest.content,\"lxml\")\n",
        "jobs = pageSource.find(\"div\",attrs={\"id\":\"search_resultsRows\"}).find_all(\"a\")\n",
        "totalgames= 0\n",
        "for job in jobs:\n",
        "    name = job.find(\"div\",attrs={\"class\":\"col search_name ellipsis\"}).text\n",
        "    date_of_publish = job.find(\"div\",attrs={\"class\":\"col search_released responsive_secondrow\"}).text\n",
        "    price = job.find(\"div\",attrs={\"class\":\"col search_price_discount_combined responsive_secondrow\"}).text\n",
        "    totalgames += 1\n",
        "    print(name,date_of_publish,price,sep=\"\\n\")\n",
        "    print(\"-\"*70)\n",
        "\n",
        "\n",
        "     "
      ],
